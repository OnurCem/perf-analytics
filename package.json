{
  "name": "perf-analytics",
  "version": "1.0.0",
  "description": "PerfAnalytics API",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "build": "tsc",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{js,ts,json,md}'",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "pretest": "npm run build",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles --testTimeout=10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OnurCem/perf-analytics.git"
  },
  "keywords": [
    "performance",
    "metrics"
  ],
  "author": {
    "name": "Onur Cem Senel",
    "email": "onurcemsenel@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OnurCem/perf-analytics/issues"
  },
  "homepage": "https://github.com/OnurCem/perf-analytics#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
